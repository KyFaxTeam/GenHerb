{
    "version": 2,
    "builds": [
      {
        "src": "./build/app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "./build/(.*)",
        "dest": "./build/app.js"
      }
    ],
    "rewrites": [{ "source": "/build/(.*)", "destination": "/build" }]
  }